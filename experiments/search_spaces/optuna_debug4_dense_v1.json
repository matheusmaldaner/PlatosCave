{
  "name": "optuna_debug4_dense_v1",
  "description": "Stage-1 dense Optuna search on debug4 cache over global node metric weights, edge combine weights, propagation settings, and graph-head weights. Excludes full role-specific node_q table to keep dimensionality tractable for 1024 trials.",
  "objective_metric": "auc_good_vs_bad",
  "direction": "maximize",
  "normalize_metric_weights": true,
  "include_component_means": true,
  "save_top_k_per_paper": 5,
  "sampler": {
    "type": "tpe",
    "seed": 0,
    "n_startup_trials": 128,
    "multivariate": true,
    "constant_liar": true
  },
  "pruner": {
    "type": "median",
    "n_startup_trials": 64,
    "n_warmup_steps": 16,
    "interval_steps": 4
  },
  "params": {
    "metric_w.credibility": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },
    "metric_w.relevance": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },
    "metric_w.evidence_strength": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },
    "metric_w.method_rigor": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },
    "metric_w.reproducibility": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },
    "metric_w.citation_support": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },

    "edge_w.role_prior": { "type": "float", "low": 0.0, "high": 0.5, "step": 0.05 },
    "edge_w.parent_quality": { "type": "float", "low": 0.0, "high": 0.5, "step": 0.05 },
    "edge_w.child_quality": { "type": "float", "low": 0.0, "high": 0.5, "step": 0.05 },
    "edge_w.alignment": { "type": "float", "low": 0.0, "high": 0.3, "step": 0.05 },
    "edge_w.synergy": { "type": "float", "low": 0.0, "high": 0.5, "step": 0.05 },

    "penalty.agg": { "type": "categorical", "choices": ["min", "mean", "softmin", "dampmin"] },
    "penalty.alpha": { "type": "float", "low": 0.25, "high": 2.0, "step": 0.25 },
    "penalty.eta": { "type": "float", "low": 0.6, "high": 1.0, "step": 0.05 },
    "penalty.softmin_beta": { "type": "float", "low": 2.0, "high": 12.0, "step": 1.0 },
    "penalty.dampmin_lambda": { "type": "float", "low": 0.1, "high": 0.8, "step": 0.05 },

    "graph_w.bridge_coverage": { "type": "float", "low": 0.0, "high": 0.5, "step": 0.05 },
    "graph_w.best_path": { "type": "float", "low": 0.0, "high": 0.5, "step": 0.05 },
    "graph_w.redundancy": { "type": "float", "low": 0.0, "high": 0.3, "step": 0.05 },
    "graph_w.fragility": { "type": "float", "low": -0.3, "high": 0.0, "step": 0.05 },
    "graph_w.coherence": { "type": "float", "low": 0.0, "high": 0.3, "step": 0.05 },
    "graph_w.coverage": { "type": "float", "low": 0.0, "high": 0.3, "step": 0.05 }
  },
  "constraints": [
    {
      "type": "sum_lte",
      "params": [
        "edge_w.role_prior",
        "edge_w.parent_quality",
        "edge_w.child_quality",
        "edge_w.alignment",
        "edge_w.synergy"
      ],
      "value": 1.0
    },
    {
      "type": "sum_gte",
      "params": [
        "edge_w.role_prior",
        "edge_w.parent_quality",
        "edge_w.child_quality",
        "edge_w.alignment",
        "edge_w.synergy"
      ],
      "value": 0.35
    },
    {
      "type": "sum_abs_lte",
      "params": [
        "graph_w.bridge_coverage",
        "graph_w.best_path",
        "graph_w.redundancy",
        "graph_w.fragility",
        "graph_w.coherence",
        "graph_w.coverage"
      ],
      "value": 1.1
    },
    {
      "type": "sum_abs_gte",
      "params": [
        "graph_w.bridge_coverage",
        "graph_w.best_path",
        "graph_w.redundancy",
        "graph_w.fragility",
        "graph_w.coherence",
        "graph_w.coverage"
      ],
      "value": 0.7
    }
  ]
}
